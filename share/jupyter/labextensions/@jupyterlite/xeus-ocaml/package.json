{
  "name": "@jupyterlite/xeus-ocaml",
  "version": "0.1.0",
  "description": "A JupyterLab extension to execute UI commands and control the Inspector from kernel comm messages.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/davy39/xeus-ocaml",
  "bugs": {
    "url": "https://github.com/davy39/xeus-ocaml/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Davy Cottet",
    "email": "davy@ik.me"
  },
  "files": [
    "lib/**/*.{d.ts,js,js.map}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/davy39/xeus-ocaml.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf ../share/jupyter/labextensions/@jupyterlite/xeus-ocaml",
    "prepare": "jlpm run clean && jlpm run build:prod",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "~4.4.0",
    "@jupyterlab/completer": "~4.4.0",
    "@jupyterlab/inspector": "~4.4.0",
    "@jupyterlab/notebook": "~4.4.0",
    "@jupyterlab/rendermime": "~4.4.0",
    "@lumino/coreutils": "^2.1.0",
    "@lumino/signaling": "^2.1.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "~4.4.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "typescript": "~5.3.0"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "../share/jupyter/labextensions/@jupyterlite/xeus-ocaml",
    "_build": {
      "load": "static/remoteEntry.ba277f8cce46924f1c7e.js",
      "extension": "./extension"
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-prettier"
    ]
  }
}
